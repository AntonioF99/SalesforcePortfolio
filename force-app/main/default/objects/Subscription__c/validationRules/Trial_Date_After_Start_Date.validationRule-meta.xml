<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Trial_Date_After_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  NOT(ISBLANK(Trial_End_Date__c)),
  Trial_End_Date__c &lt; Start_Date__c
)</errorConditionFormula>
    <errorMessage>Trial End Date must be after Start Date</errorMessage>
</ValidationRule>
